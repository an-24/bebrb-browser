<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.*?>

<VBox fx:id="root" layoutX="0.0" layoutY="0.0" minHeight="-1.0" minWidth="200.0" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" styleClass="dialogbox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.DialogController">
  <children>
    <HBox alignment="CENTER_RIGHT" spacing="5.0" style="-fx-background-color: transparent;">
      <Button fx:id="btnCancel" cancelButton="true" mnemonicParsing="false" text="Cancel" />
      <Button fx:id="btnOk" defaultButton="true" mnemonicParsing="false" text="Ok" />
    </HBox>
    <VBox fx:id="errorBox" prefHeight="-1.0" prefWidth="-1.0" />
    <AnchorPane fx:id="apContent" prefHeight="-1.0" prefWidth="-1.0" styleClass="innerbox" />
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
